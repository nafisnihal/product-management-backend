# Backend Environment Variables
# Copy this to .env file in your backend project

NODE_ENV="production"
PORT="5000"
FRONTEND_URL="https://your-frontend-app.vercel.app"
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"
JWT_EXPIRES_IN="7d"

